{
  "manifest_version": 2,
  "name": "GCM Example MinUI",
  "description": "Let the server notify YOU",
  "version": "1.4",
  "background": {
    "page": "background.html"
  },
  "permissions": [ 
    "*://localhost/*",
    "*://*.appspot.com/*",
    "pushMessaging"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
